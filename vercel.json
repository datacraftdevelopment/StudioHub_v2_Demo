{
  "version": 2,
  "build": {
    "env": {
      "NEXT_PUBLIC_DEMO_MODE": "true"
    }
  },
  "env": {
    "NEXT_PUBLIC_DEMO_MODE": "true",
    "NODE_ENV": "production"
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=300, stale-while-revalidate=600"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ]
}
